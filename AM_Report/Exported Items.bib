@article{strouboulis_p-version_2004,
	title = {p-version of the generalized {FEM} using mesh-based handbooks with applications to multiscale problems},
	volume = {60},
	issn = {1097-0207},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/nme.1017/abstract},
	doi = {10.1002/nme.1017},
	abstract = {In this paper, we analyse the p-convergence of a new version of the generalized finite element method (generalized {FEM} or {GFEM}) which employs mesh-based handbook functions which are solutions of boundary value problems in domains extracted from vertex patches of the employed mesh and are pasted into the global approximation by the partition of unity method ({PUM}). We show that the p-version of our {GFEM} is capable of achieving very high accuracy for multiscale problems which may be impossible to solve using the standard {FEM}. We analyse the effect of the main factors affecting the accuracy of the method namely: (a) The data and the buffer included in the handbook domains, and (b) The accuracy of the numerical construction of the handbook functions. We illustrate the robustness of the method by employing as model problem the Laplacian in a domain with a large number of closely spaced voids. Similar robustness can be expected for problems of heat-conduction and elasticity set in domains with a large number of closely spaced voids, cracks, inclusions, etc. Copyright © 2004 John Wiley \& Sons, Ltd.},
	pages = {1639--1672},
	number = {10},
	journaltitle = {Int. J. Numer. Meth. Engng.},
	author = {Strouboulis, Theofanis and Zhang, Lin and Babuška, Ivo},
	urldate = {2016-05-11},
	date = {2004-07-14},
	langid = {english},
	keywords = {generalized finite element method ({GFEM}), mesh-based handbook functions, partition of unity method ({PUM})},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/I7SRB7S5/Strouboulis et al. - 2004 - p-version of the generalized FEM using mesh-based .pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/P6PU7QMA/abstract.html:text/html}
}

@article{babuska_partition_1997,
	title = {The Partition of Unity Method},
	volume = {40},
	issn = {1097-0207},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/(SICI)1097-0207(19970228)40:4<727::AID-NME86>3.0.CO;2-N/abstract},
	doi = {10.1002/(SICI)1097-0207(19970228)40:4<727::AID-NME86>3.0.CO;2-N},
	abstract = {A new finite element method is presented that features the ability to include in the finite element space knowledge about the partial differential equation being solved. This new method can therefore be more efficient than the usual finite element methods. An additional feature of the partition-of-unity method is that finite element spaces of any desired regularity can be constructed very easily. This paper includes a convergence proof of this method and illustrates its efficiency by an application to the Helmholtz equation for high wave numbers. The basic estimates for a posteriori error estimation for this new method are also proved. © 1997 by John Wiley \& Sons, Ltd.},
	pages = {727--758},
	number = {4},
	journaltitle = {Int. J. Numer. Meth. Engng.},
	author = {Babuška, I. and Melenk, J. M.},
	urldate = {2016-05-11},
	date = {1997-02-28},
	langid = {english},
	keywords = {finite element method, finite element methods for highly oscillatory solutions, meshless finite element method},
	file = {Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/CTRWTZRN/abstract.html:text/html}
}

