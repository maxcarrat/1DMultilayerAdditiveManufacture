	author = {Šolín, Pavel and Červený, Jakub and Dubcova, L. and Andrs, D.},
	year = {2010-08},
	note = {bibtex: Solin2010},
	keywords = {Adaptive multimesh hp-{FEM}, citable, hp-adaptivity, linear elasticity, Monolithic discretization, thermoelasticity},
	file = {Solin et al_2010_Monolithic discretization of linear thermoelasticity problems via adaptive.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/DTKES84T/Solin et al_2010_Monolithic discretization of linear thermoelasticity problems via adaptive.pdf:application/pdf}
