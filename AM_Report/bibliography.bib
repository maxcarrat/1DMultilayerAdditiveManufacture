@article{szymczak_anisotropic_2011,
	title = {Anisotropic 2D mesh adaptation in hp-adaptive {FEM}},
	volume = {4},
	issn = {1877-0509},
	url = {http://www.sciencedirect.com/science/article/pii/S1877050911002559},
	doi = {10.1016/j.procs.2011.04.197},
	series = {Proceedings of the International Conference on Computational Science, {ICCS} 2011},
	abstract = {The paper presents a grammar for anisotropic two-dimensional mesh adaptation in hp-adaptive Finite Element Method with rectangular elements. It occurs that a straightforward approach to modeling this process via grammar productions leads to potential deadlock in h-adaptation of the mesh. This fact is shown on a Petri net model of an exemplary adaptation. Therefore auxiliary productions are added to the grammar in order to ensure that any sequence of productions allowed by the grammar does not lead to a deadlock state. The fact that the enhanced grammar is deadlock-free is proven via a corresponding Petri net model. The proof has been performed by means of reachability graph construction and analysis. The paper is enhanced with numerical simulations of magnetolluric measurements where the deadlock problem occured.},
	pages = {1818--1827},
	journal = {Procedia Computer Science},
	author = {Szymczak, Arkadiusz and Paszýnska, Anna and Paszýnski, Maciej and Pardo, David},
	urlyear = {2014-06-05},
