	keywords = {collision detection and modeling, Contact, eulerian},
	file = {Faure et al_2007_Eulerian contact for versatile collision processing.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/75W37NAM/Faure et al_2007_Eulerian contact for versatile collision processing.pdf:application/pdf;Faure et al_2007_Eulerian contact for versatile collision processing.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/5H2IEACB/Faure et al_2007_Eulerian contact for versatile collision processing.pdf:application/pdf}
