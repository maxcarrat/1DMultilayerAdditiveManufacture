	year = {2013-09-01},
	note = {bibtex: Rosolen2013},
	keywords = {High-fidelity geometry, Isogeometric analysis, Local refinement, Max-ent approximants, {NEFEM}, Volume discretization},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/Z9CR6MWQ/Rosolen and Arroyo - 2013 - Blending isogeometric analysis and local maximum e.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/744593KA/S0045782513001321.html:text/html}
