@article{wang_adaptive_2013,
	title = {An adaptive refinement approach for topology optimization based on separated density field description},
	volume = {117},
	issn = {0045-7949},
	url = {http://www.sciencedirect.com/science/article/pii/S0045794912002684},
	doi = {10.1016/j.compstruc.2012.11.004},
	abstract = {This paper presents an adaptive density point refinement approach for continuum topology optimization on the basis of an analysis-mesh separated material density field description based on nodal design variables. The Shepard interpolants are used to construct a strictly range-restricted density field over the design domain with the density design variables defined on a density point grid. Since the density points are defined independent of the finite element mesh, it is easy to refine the density point grid without remeshing the finite element model. A refinement criterion is given to identify the gray transitional regions to be adaptively refined in the subsequent optimization iterations. With such a refinement scheme, the topology optimization can start from a relatively coarse density point grid but still yields a desired higher resolution of the structural boundaries in the final design. Because refinements are only performed when and where necessary, this method is able to improve the boundary description quality of the optimal result with much less design variables as compared with the case of global refinement, and therefore can greatly reduce the computational burden involved in the sensitivity analysis and optimization process. Moreover, the percentage of transitional regions in the final solutions can also be reduced. Compared with using a uniformly globally-dense density point arrangement, this approach can achieve similar optimal designs but with much less computational cost. Numerical examples are given to demonstrate the effectiveness and efficiency of the present approach.},
	pages = {10--22},
	journal = {Computers \& Structures},
	author = {Wang, Yiqiang and Kang, Zhan and He, Qizhi},
	urlyear = {2016-10-25},
	year = {2013-02},
	keywords = {Adaptive refinement, Boundary description quality, Gray elements, Mesh-independent density interpolation, Nodal density, Topology optimization},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/QTQBE52Q/Wang et al. - 2013 - An adaptive refinement approach for topology optim.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/ERADEBHM/S0045794912002684.html:text/html}
}

