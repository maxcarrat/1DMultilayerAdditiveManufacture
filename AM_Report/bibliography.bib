	keywords = {arbitrary shape, equivalent inclusion method, fast Fourier transform, inhomogeneity, inhomogeneous inclusion, three-dimensional},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/6QV7HG7H/Zhou et al. - 2011 - Semi-analytic solution for multiple interacting th.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/W4HSTNXC/abstract.html:text/html}
