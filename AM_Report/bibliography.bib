	urlyear = {2016-07-12},
	year = {2015-08-10},
	langid = {english},
	note = {bibtex: Ramos2015},
	keywords = {Dirichlet boundary conditions, {GFEM}, {IGFEM}, Lagrange multiplier method, Non-matching meshes, Xfem},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/SVCJ98ZN/Ramos et al. - 2015 - A new formulation for imposing Dirichlet boundary .pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/IWU22SQ3/abstract.html:text/html}
}

@book{johnson_numerical_2009,
