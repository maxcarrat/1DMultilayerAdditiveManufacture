	volume = {25},
	issn = {10648275},
	url = {http://link.aip.org/link/?SJOCE3/25/324/1 http://link.aip.org/link/SJOCE3/v25/i1/p324/s1&Agg=doi http://epubs.siam.org/doi/pdf/10.1137/S1064827502405318},
