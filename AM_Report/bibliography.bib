@thesis{niggl_tragwerksanalyse_2007,
	title = {Tragwerksanalyse am volumenorientierten Gesamtmodell},
	url = {https://mediatum.ub.tum.de/node?id=632067},
	abstract = {Computermodelle im konstruktiven Ingenieurbau sind häufig nur für spezielle Aufgabenbereiche konzipiert. Es existiert eine Vielzahl von fachspezifischen Teilmodellen, was die Konsistenthaltung und den Austausch von Informationen erschwert. Die Arbeit schlägt vor, Arbeitsprozesse der Tragwerksplanung auf strikt volumenorientierte Modelle zu gründen. Um eine statische Analyse durchzuführen, soll ein Tragwerk nicht mehr in dimensionsreduzierte Strukturen, wie Balken, Platten oder Stabwerke eingeteilt werden. Es soll stattdessen für alle Aufgaben ein einheitlich volumenorientierter Ansatz basierend auf finiten Elementen hoher Ordnung zum Einsatz kommen. Ziel der Arbeit ist, dieses Modell zu beschreiben, sich daraus ergebende Fragen zu diskutieren und Lösungsmöglichkeiten aufzuzeigen. Ein besonderes Augenmerk liegt auf der Frage, inwieweit sich volumenorientierte Berechnungsergebnisse zur Bemessung von Stahlbetonbauteilen nutzen lassen und welche Vorteile sich daraus ergeben.},
	institution = {Technische Universität München},
