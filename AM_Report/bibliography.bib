	year = {2011-09},
	note = {00023 
bibtex: Wirth2011},
	keywords = {Bone–implant competence, Bone microstructure, Mechanical testing, Micro-finite element (μ{FE}) analysis, Peri-implant bone quality, Proximal humerus},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/6ZX5UGZN/Wirth et al. - 2011 - Implant stability is affected by local bone micros.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/GDSP4BMI/S8756328211009756.html:text/html}
