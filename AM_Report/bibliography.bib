@article{betz_numerical_2014,
	title = {Numerical methods for the discretization of random fields by means of the Karhunen–Loève expansion},
	volume = {271},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/S0045782513003502},
	doi = {10.1016/j.cma.2013.12.010},
	abstract = {The computational efficiency of random field representations with the Karhunen–Loève ({KL}) expansion relies on the solution of a Fredholm integral eigenvalue problem. This contribution compares different methods that solve this problem. Focus is put on methods that apply to arbitrary shaped domains and arbitrary autocovariance functions. These include the Nyström method as well as collocation and Galerkin projection methods. Among the Galerkin methods, we investigate the finite element method ({FEM}) and propose the application of the finite cell method ({FCM}). This method is based on an extension to the {FEM} but avoids mesh generation on domains of complex geometric shape. The {FCM} was originally presented in Parvizian et al. (2007) [17] for the solution of elliptic boundary value problems. As an alternative to the L 2 -projection of the covariance function used in the Galerkin method, H 1 / 2 -projection and discrete projection are investigated. It is shown that the expansion optimal linear estimation ({EOLE}) method proposed in Li and Der Kiureghian (1993) [18] constitutes a special case of the Nyström method. It is found that the {EOLE} method is most efficient for the numerical solution of the {KL} expansion. The {FEM} and the {FCM} are more efficient than the {EOLE} method in evaluating a realization of the random field and, therefore, are suitable for problems in which the time spent in the evaluation of random field realizations has a major contribution to the overall runtime – e.g., in finite element reliability analysis.},
	pages = {109--129},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Betz, Wolfgang and Papaioannou, Iason and Straub, Daniel},
	urlyear = {2016-08-10},
	year = {2014-04-01},
	note = {bibtex: Betz2014},
	keywords = {Collocation method, Finite Cell Method, Galerkin method, Karhunen–Loève expansion, Nyström method, Random field discretization},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/MDNFVIJC/Betz et al. - 2014 - Numerical methods for the discretization of random.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/EJ39999X/S0045782513003502.html:text/html}
}

