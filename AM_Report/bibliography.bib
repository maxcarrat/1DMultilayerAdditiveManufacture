@article{solin_adaptive_2010-1,
	title = {Adaptive -{FEM} with dynamical meshes for transient heat and moisture transfer problems},
	volume = {233},
	issn = {0377-0427},
	url = {http://www.sciencedirect.com/science/article/pii/S0377042709004282},
	doi = {10.1016/j.cam.2009.07.025},
	series = {Finite Element Methods in Engineering and Science ({FEMTEC} 2009)},
	abstract = {We are concerned with the time-dependent multiphysics problem of heat and moisture transfer in the context of civil engineering applications. The problem is challenging due to its multiscale nature (temperature usually propagates orders of magnitude faster than moisture), different characters of the two fields (moisture exhibits boundary layers which are not present in the temperature field), extremely long integration times (30 years or more), and lack of viable error control mechanisms. In order to solve the problem efficiently, we employ a novel multimesh adaptive higher-order finite element method ( h p -{FEM}) based on dynamical meshes and adaptive time step control. We investigate the possibility to approximate the temperature and humidity fields on individual dynamical meshes equipped with mutually independent adaptivity mechanisms. Numerical examples related to a realistic nuclear reactor vessel simulation are presented.},
	pages = {3103--3112},
	number = {12},
