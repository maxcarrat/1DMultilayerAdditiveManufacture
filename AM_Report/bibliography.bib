	note = {00018 
bibtex: Park2012a},
	keywords = {adaptive mesh coarsening ({AMC}), adaptive mesh refinement ({AMR}), adaptive mesh refinement and coarsening ({AMR}\&C), dynamic cohesive fracture, extrinsic cohesive zone model, {PPR} potential-based model, topological data structure ({TopS})},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/IF7V3865/Park et al. - 2012 - Adaptive mesh refinement and coarsening for cohesi.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/7CGUWAQD/abstract.html:text/html}
}

