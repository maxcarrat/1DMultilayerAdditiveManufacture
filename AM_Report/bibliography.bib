	year = {2015-08-15},
	note = {bibtex: Rothe2015},
	keywords = {Differential–algebraic equations, Fully coupled finite elements, Monolithic approach, Partitioned approach, Thermo-viscoplasticity},
	file = {Rothe et al. - 2015 - Monolithic and partitioned coupling schemes for th.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/KSJA6S39/Rothe et al. - 2015 - Monolithic and partitioned coupling schemes for th.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/RBPJZ45K/S0045782515001681.html:text/html}
