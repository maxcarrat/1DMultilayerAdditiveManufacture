@thesis{wassouf_mortar_2010,
	title = {Die Mortar Methode für Finite Elemente hoher Ordnung},
	abstract = {This thesis investigates a non-continuous discretization strategy in the context of the high order Finite Element Method (p-{FEM}) for 3D elastostatic analyses. The approach is known as a Mortar Element Method, which we use to couple hexahedral and tetrahedral elements. The tetrahedron’s shape functions developed in this work are based on a degenerated hexahe- dral to tetrahedral mapping called Duffy transformation. They consist of integrated Legendre polynomials exhibiting a tensor product structure on the hexahedron which allows for fast integration algorithms. Numerical experiments confirm theoretical predictions stating that for smooth problems an exponential convergence rate is obtained under pure p-refinement. For other types of problems at least algebraic rates of convergence are achieved. Furthermore, different discretizations of the shape functions used for the Lagrangian multipliers on the in- terface of the two structures are investigated. Here, non-continuous ansatz spaces produce the best results for the examples under investigation. Even for nonlinear material behaviour a numerical experiment provides a sound indication of the robust properties of the proposed method.},
	pagetotal = {154},
	institution = {Technische Universität München},
	type = {phdthesis},
	author = {Wassouf, Ziad},
	year = {2010},
	note = {bibtex: Wassouf2010},
	keywords = {high-order {FEM}, mortar},
	file = {Wassouf_2010_Die Mortar Methode für Finite Elemente hoher Ordnung.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/CWW27SRG/Wassouf_2010_Die Mortar Methode für Finite Elemente hoher Ordnung.pdf:application/pdf}
