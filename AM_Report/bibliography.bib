	keywords = {{eXtended} Finite Element Method, fracture, generalized finite element method, high-order approximations},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/UMMDP2ZR/Pereira et al. - 2009 - hp-Generalized FEM and crack surface representatio.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/9E8FBZI7/abstract.html:text/html}
