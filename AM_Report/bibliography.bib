	langid = {english},
	note = {bibtex: zotero-1140216-5758},
	keywords = {generalized finite element method, proper generalized decomposition, transient problem, vademecum-based generalized finite element method, welding simulation},
	file = {Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/RTIDQ462/abstract.html:text/html}
