	note = {bibtex: Andreykiv2012},
	keywords = {{bogToCite}, Contact, Finite element method, implant positioning, Lagrange multipliers, level set},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/8WHTBEBF/Andreykiv et al. - 2012 - A level-set-based large sliding contact algorithm .pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/DZP7RDR7/abstract\;jsessionid=1E9EB9C82943FC27EFFE00F0C8EF1200.html:text/html}
