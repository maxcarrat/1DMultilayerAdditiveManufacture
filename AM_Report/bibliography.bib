	keywords = {adaptivity, cmi, corresponding author, elliptic problems, email address, embedded boundary conditions, embedded domain methods, fat boundary, Fictitious Domain Method, fr, immersed boundary, interface method, isabelle ramière, local adaptive mesh refinement, multigrid, ramiere, spread, univ-mrs, weak boundary conditions},
