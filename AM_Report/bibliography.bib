	keywords = {Adaptive methods, A posteriori error bounds, Convection–diffusion problems, Elliptic boundary value problems, h – p finite element method},
	file = {ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/HXZXA2PV/S016892749900046X.html:text/html}
}

