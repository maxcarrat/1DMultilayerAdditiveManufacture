	keywords = {Algorithms, anisotropy, Biomechanical Phenomena, Biomedical Engineering, Bone, Bone and Bones, Computer Simulation, Equipment Design, Finite element analysis, Finite element method, Fractures, Bone, Humans, Image Processing, Computer-Assisted, implants, Materials Testing, Models, Theoretical, Orthopedics, patient-specific, Prosthesis Design, Stress, Mechanical, Tomography, X-Ray Computed},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/JEZ9EM8F/Poelert et al. - 2013 - Patient-specific finite element modeling of bones.pdf:application/pdf;Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/TJKRNT84/Poelert et al. - 2013 - Patient-specific finite element modeling of bones.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/8MQXTJTJ/464.html:text/html;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/N2TGUCEA/464.html:text/html}
