@article{puso_mortar_2004,
	title = {A mortar segment-to-segment contact method for large deformation solid mechanics},
	volume = {193},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/S0045782503005802},
	doi = {10.1016/j.cma.2003.10.010},
	abstract = {Contact modeling is still one of the most difficult aspects of non-linear implicit structural analysis. Most 3D contact algorithms employed today use node-on-segment approaches for contacting dissimilar meshes. Two pass node-on-segment contact approaches have the well known deficiency of locking due to over-constraint. In this work, we develop and demonstrate a segment-to-segment contact approach based on the mortar method. By eliminating the over-constraint, this method appears to be much more robust than the node-on-segment approach. Furthermore, node-on-segment approaches suffer when individual nodes slide out of contact at contact surface boundaries or when contacting nodes slide from facet to facet. This causes jumps in the contact forces due to the discrete nature of the constraint enforcement and difficulties in convergence for implicit solution techniques. These jumps in forces are avoided in the segment-to-segment approach, since penetration is measured by the occluded weighted volume which varies smoothly with perturbations.
Until now, all mortar schemes have been developed for small deformation/sliding and have mainly been applied on flat surfaces. Here we will present the integration scheme and the linearization approach used for handling the contact surface integrals in the event of large sliding. The schemes are designed to conserve linear momentum and can be applied to arbitrarily curved 3D surfaces. Although expensive, our numerical examples demonstrate that the approach is far more robust than even smoothed node-on-segment contact techniques. That is, problems previously not solvable with node-on-segment can now be solved with our segment-to-segment contact.},
	pages = {601--629},
	number = {6},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Puso, Michael A. and Laursen, Tod A.},
	urlyear = {2014-05-21},
	year = {2004-02-13},
	note = {bibtex: Puso2004},
	keywords = {Contact, Finite elements, large deformation, mortar method},
	file = {Puso and Laursen - 2004 - A mortar segment-to-segment contact method for lar.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/2FZKH3XR/Puso and Laursen - 2004 - A mortar segment-to-segment contact method for lar.pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/QRRHU7C5/S0045782503005802.html:text/html}
}

@thesis{koschade_finite_2013,
	location = {Munich},
	title = {The Finite Cell Method for thin walled structures},
	abstract = {The Finite Cell Method belongs to the category of embedded domain methods
and hence allows for the numerical solution of a partial differential equation on
a domain Ω without the necessary evil of finding a suitable mesh first. For thin
walled structures it is known to behave ill-conditioned and to yield inaccurate
solutions, most likely due to it’s particular susceptibility towards errors in the
numerical evaluation of the integral expressions. The objective of this thesis is to
introduce a precise integration scheme applicable to a specific, simple test case
for a 2D linear elastic problem and to subsequently study the behaviour and
convergence properties of the Finite Cell Method. Eventually a prospect is given
which strategies could be pursued in the future to enable the reliable disposition
of the Finite Cell Method for thin walled structures.},
	institution = {Chair for Computation in Engineering, Technische Universität München},
	type = {Bachelor's Thesis},
	author = {Koschade, Maximilian},
	year = {2013},
	note = {bibtex: Koschade2013},
	keywords = {Finite Cell Method}
}

