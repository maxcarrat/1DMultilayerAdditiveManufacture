@article{sudhakar_accurate_2014,
	title = {An accurate, robust, and easy-to-implement method for integration over arbitrary polyhedra: Application to embedded interface methods},
	volume = {273},
	issn = {0021-9991},
	url = {http://www.sciencedirect.com/science/article/pii/S0021999114003659},
	doi = {10.1016/j.jcp.2014.05.019},
	shorttitle = {An accurate, robust, and easy-to-implement method for integration over arbitrary polyhedra},
	abstract = {We present an accurate method for the numerical integration of polynomials over arbitrary polyhedra. Using the divergence theorem, the method transforms the domain integral into integrals evaluated over the facets of the polyhedra. The necessity of performing symbolic computation during such transformation is eliminated by using one dimensional Gauss quadrature rule. The facet integrals are computed with the help of quadratures available for triangles and quadrilaterals. Numerical examples, in which the proposed method is used to integrate the weak form of the Navier–Stokes equations in an embedded interface method ({EIM}), are presented. The results show that our method is as accurate and generalized as the most widely used volume decomposition based methods. Moreover, since the method involves neither volume decomposition nor symbolic computations, it is much easier for computer implementation. Also, the present method is more efficient than other available integration methods based on the divergence theorem. Efficiency of the method is also compared with the volume decomposition based methods and moment fitting methods. To our knowledge, this is the first article that compares both accuracy and computational efficiency of methods relying on volume decomposition and those based on the divergence theorem.},
	pages = {393--415},
	journal = {Journal of Computational Physics},
	author = {Sudhakar, Y. and Moitinho de Almeida, J. P. and Wall, Wolfgang A.},
	urlyear = {2015-10-23},
	year = {2014-09-15},
	note = {bibtex: Sudhakar2014},
	keywords = {Complex volumes, Divergence theorem, Embedded interface method, Enriched partition of unity method, {eXtended} Finite Element Method, Finite Cell Method, Numerical integration, Polyhedra},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/75VQ35XX/Sudhakar et al. - 2014 - An accurate, robust, and easy-to-implement method .pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/D4AAH98T/S0021999114003659.html:text/html}
}

