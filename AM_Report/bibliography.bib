	year = {2016-01-01},
	langid = {english},
	note = {bibtex: Varduhn2016},
	keywords = {adaptive tetrahedral meshes, Finite Cell Method, higher-order finite element methods, Immersogeometric analysis},
	file = {Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/RHNTTCJD/Varduhn et al. - 2016 - The tetrahedral finite cell method Higher-order i.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/FJ8974AW/abstract.html:text/html}
