	author = {Soghrati, Soheil and Ahmadian, Hossein},
	urlyear = {2016-07-07},
	year = {2015-10-15},
	note = {bibtex: Soghrati2015b},
	keywords = {Composite material, Hierarchical enrichment, {HIFEM}, materials interface, Mesh-independent, Steiner point},
	file = {ScienceDirect Full Text PDF:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/6USRCPPQ/Soghrati and Ahmadian - 2015 - 3D hierarchical interface-enriched finite element .pdf:application/pdf;ScienceDirect Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/8D7KNUCW/S0021999115004337.html:text/html}
}

@thesis{ledentsov_model_2010-1,
	location = {Munich},
	title = {Model adaptivity in sheet metal forming simulation},
	url = {https://mediatum.ub.tum.de/?id=736719},
	abstract = {Sheet metal forming is typically simulated using shell finite elements. These elements are computationally very efficient. However, their reduced kinematics is not sufficient to represent complex three-dimensional stress states. Solid finite elements provide a better approximation, yet at a significantly higher computational cost. This thesis presents a computational framework, in which the benefits of both element classes are combined  using solid elements only locally where the shell elements are insufficiently precise. The rest of the computational domain is discretized using shell elements. A mechanically motivated model error indicator is introduced to adaptively select appropriate elements. Results of these computations show very good agreement with the purely solid discretization, however, at a significantly lower computational cost.},
	institution = {Technische Universität München},
