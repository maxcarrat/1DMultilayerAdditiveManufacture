@article{s._reese_stabilization_2000,
	title = {A stabilization technique to avoid hourglassing in finite elasticity},
	volume = {48},
	url = {http://onlinelibrary.wiley.com/store/10.1002/(SICI)1097-0207(20000510)48:1%3C79::AID-NME869%3E3.0.CO;2-D/asset/869_ftp.pdf?v=1&t=hir3qr1r&s=d821dccdf590939880b08c5483d151f12a33bb8d},
	abstract = {Enhanced strain element formulations are known to show an outstanding performance in many applications.
The stability of these elements, however, cannot be guaranteed for general deformation states and arbitrarily
shaped elements. In order to overcome this deÿciency, we develop an innovative control technique based
on a modal analysis on element level. The control is completely automatic in the sense that no artiÿcial
factors are introduced. The computational e ort is negligible. The key to the approach is the split of the
element tangent matrix into constant and hourglass parts which is not possible for the classical enhanced
strain concept in general. This motivates the use of a recently developed reduced integration method, which,
since its stabilization part is derived on the basis of the enhanced strain method, shows the same performance
and retains the crucial split. Using this formulation in combination with the new control technique, leads to
a ‘smart’ element which is free of hourglass instabilities and generally applicable, also for strongly distorted
meshes. Copyright ? 2000 John Wiley \& Sons, Ltd.},
	pages = {79--109},
	journal = {Int. J. Numer. Meth. Engng.},
	author = {{S. Reese} and {P. Wriggers}},
	urlyear = {2013-07-05},
	year = {2000},
	note = {bibtex: S.Reese2000},
	keywords = {distorted meshes, enhanced strain method, hourglass stabilization, one Gauss point integration, plane strain state, smart elements},
	file = {S. Reese_P. Wriggers_2000_A stabilization technique to avoid hourglassing in ÿnite elasticity.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/N23EUBDC/S. Reese_P. Wriggers_2000_A stabilization technique to avoid hourglassing in ÿnite elasticity.pdf:application/pdf}
