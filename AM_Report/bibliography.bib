	note = {00034 
bibtex: Mayer2009},
	keywords = {constrained Delaunay tetrahedralization, exact numerical integration, Fluid–structure interaction, interface localization, surface–surface intersection, three-dimensional higher-order extended finite element method ({XFEM})},
	file = {Mayer_et_al-2009-International_Journal_for_Numerical_Methods_in_Engineering.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/9DC4MB47/Mayer_et_al-2009-International_Journal_for_Numerical_Methods_in_Engineering.pdf:application/pdf;Snapshot:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/3M4D9Q94/abstract.html:text/html}
