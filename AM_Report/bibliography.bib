  urlyear = {2016-05-11},
  year = {2004-07-14},
  pages = {1639--1672},
  keywords = {generalized finite element method (GFEM),mesh-based handbook functions,partition of unity method (PUM)},
  file = {Full Text PDF:/home/massimo/.mozilla/firefox/j8jwhfs5.default/zotero/storage/I7SRB7S5/Strouboulis et al. - 2004 - p-version of the generalized FEM using mesh-based .pdf:application/pdf;Snapshot:/home/massimo/.mozilla/firefox/j8jwhfs5.default/zotero/storage/P6PU7QMA/abstract.html:text/html}
