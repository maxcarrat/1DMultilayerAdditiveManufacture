@article{blanchard_intravoxel_2013,
	title = {Intravoxel bone micromechanics for {microCT}-based finite element simulations},
	volume = {46},
	issn = {1873-2380},
	doi = {10.1016/j.jbiomech.2013.06.036},
	abstract = {While micro-{FE} simulations have become a standard tool in computational biomechanics, the choice of appropriate material properties is still a relevant topic, typically involving empirical grey value-to-elastic modulus relations. We here derive the voxel-specific volume fractions of mineral, collagen, and water, from tissue-independent bilinear relations between mineral and collagen content in extracellular bone tissue (J. Theor. Biol. 287: 115, 2011), and from the measured X-ray attenuation information quantified in terms of grey values. The aforementioned volume fractions enter a micromechanics representation of bone tissue, as to deliver voxel-specific stiffness tensors. In order to check the relevance of this strategy, we convert a micro Computer Tomograph of a mouse femur into a regular Finite Element mesh, apply forces related to the dead load of a standing mouse, and then compare simulation results based on voxel-specific heterogeneous elastic properties to results based on homogeneous elastic properties related to the spatial average over the solid bone matrix compartment, of the X-ray attenuation coefficients. The element-specific strain energy density illustrates that use of homogeneous elastic properties implies overestimation of the organ stiffness. Moreover, the simulation reveals large tensile normal stresses throughout the femur neck, which may explain the mouse femur neck's trabecular morphology being quite different from the human case, where the femur neck bears compressive forces and bending moments.},
	pages = {2710--2721},
	number = {15},
	journal = {J Biomech},
	author = {Blanchard, Romane and Dejaco, Alexander and Bongaers, Evi and Hellmich, Christian},
	year = {2013-10-18},
	pmid = {24016680},
	note = {00010 
bibtex: Blanchard2013},
	keywords = {Animals, Biomechanical Phenomena, Collagen, Elasticity, Femur Neck, Finite element analysis, Humans, Mice, Models, Biological, Weight-Bearing, X-Ray Microtomography},
	file = {Blanchard_Hellmich_JBM_2013.pdf:/home/ferdinando/.zotero/zotero/95ibj2bp.default/zotero/storage/KDMAC9CM/Blanchard_Hellmich_JBM_2013.pdf:application/pdf}
